{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway-api",
  "projects": {
    "New-linen-microservice": {
      "type": "application",
      "root": "apps/New-linen-microservice",
      "entryFile": "main",
      "sourceRoot": "apps/New-linen-microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/New-linen-microservice/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "email-service": {
      "type": "application",
      "root": "apps/email-service",
      "entryFile": "main",
      "sourceRoot": "apps/email-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/email-service/tsconfig.app.json"
      }
    },
    "gateway-api": {
      "type": "application",
      "root": "apps/gateway-api",
      "entryFile": "main",
      "sourceRoot": "apps/gateway-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway-api/tsconfig.app.json"
      }
    },
    "item-service": {
      "type": "application",
      "root": "apps/item-service",
      "entryFile": "main",
      "sourceRoot": "apps/item-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/item-service/tsconfig.app.json"
      }
    }
  }
}